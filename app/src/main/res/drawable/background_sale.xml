<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="32dp"
        android:top="4dp">
        <rotate android:fromDegrees="45">
            <shape>
                <solid android:color="@color/colorPrimary" />
                <size
                    android:width="14dp"
                    android:height="14dp" />
            </shape>
        </rotate>
    </item>


    <item
        android:top="1dp"
        android:bottom="1dp"
        android:right="7dp"
        android:left="11dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary" />
        </shape>
    </item>

</layer-list>